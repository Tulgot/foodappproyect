<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.EasyFood" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryVariant">@color/accent</item>
    </style>

    <style name="Theme.EasyFood" parent="Base.Theme.EasyFood" />

    <style name="Bottomnavmenu.selected.inidcator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@color/selecteditembottomnavmenu</item>
    </style>

    <style name="foodappTextMargin">
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="foodappImageMargin" parent="foodappTextMargin">
        <item name="android:layout_marginStart">25dp</item>
    </style>

    <style name="foodapptextstyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/myfont</item>
    </style>

    <style name="foodapptextstyleandmargin" parent="foodappTextMargin">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/myfont</item>
    </style>

</resources>